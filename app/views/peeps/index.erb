<h1> Peeps </h1>

<ul id='peeps'>
  <% @peeps.each do |peep| %>
    <li>
      Name: <%= peep.name %>
      Username: <%= peep.username %>
      Text: <%= peep.text %>
      Time: <%= peep.time %>
    </li>
  <% end %>
</ul>

<% if current_user %>
<form action='/peeps/new' method='POST'>
  <input type='submit' value='New Peep'>
</form>
<% end %>
